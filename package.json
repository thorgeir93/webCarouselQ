{
  "name": "judo-heroes",
  "version": "1.0.0",
  "babel": {
    "plugins": [
      "css-modules-transform"
    ]
  },
  "description": "Simple application to showcase how to achieve universal rendering and routing with React and Express.",
  "main": "src/server.js",
  "repository": "git@github.com:lmammino/judo-heroes.git",
  "repository": "git@github.com:thorgeir93/webCarouselQ",
  "scripts": {
    "start": "./node_modules/.bin/babel-node --presets=react,es2015 ./src/server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "./node_modules/.bin/http-server src/static",
    "start-dev-hmr": "./node_modules/.bin/webpack-dev-server --progress --inline --hot",
    "start-dev-hmr-2": "./node_modules/.bin/webpack-dev-server --progress --inline --hot --port 3333 --host 192.168.1.47",
    "build": "SET NODE_ENV=production ./node_modules/.bin/webpack -p"
  },
  "author": "Luciano Mammino",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.27.2",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "pg": "^6.1.5",
    "pg-promise": "^5.6.7",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.6.1",
    "style-loader": "^0.13.2",
    "xss": "^0.3.3"
  },
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel-cli": "^6.22.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "http-server": "^0.9.0",
    "postcss-color-rebeccapurple": "^2.0.1",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.5.4"
  }
}
